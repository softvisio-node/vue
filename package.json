{
    "name": "@softvisio/vue",
    "version": "5.0.0",
    "description": "Softisio Vue",
    "keywords": [
        "softvisio",
        "vue"
    ],
    "homepage": "https://github.com/softvisio/vue",
    "bugs": {
        "url": "https://github.com/softvisio/vue/issues",
        "email": "zdm@softvisio.net"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/softvisio/vue.git"
    },
    "author": "zdm <zdm@softvisio.net>",
    "type": "module",
    "imports": {
        "#core/*": "@softvisio/core/*",
        "#lib/*": "./lib/*.js"
    },
    "exports": {
        ".": "./lib/index.js",
        "./*": [
            "./lib/*.js",
            "./lib/*.vue"
        ],
        "./resources/*": "./resources/*",
        "./babel.config.cjs": "./resources/babel.config.cjs",
        "./webpack.config.js": "./resources/webpack.config.js"
    },
    "bin": {
        "run": "bin/run.js"
    },
    "dependencies": {
        "@softvisio/vuex": "^4.0.0",
        "@vue/cli-plugin-babel": "^5.0.0-beta.3",
        "@vue/cli-service": "^5.0.0-beta.3",
        "@vue/compiler-sfc": "^3.1.2",
        "vue": "^3.1.2"
    },
    "peerDependencies": {
        "@softvisio/core": "github:softvisio/core#main"
    }
}
